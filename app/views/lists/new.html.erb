<h1> New list <h1>
<%= simple_form_for(@list, method: :post) do |f| %>
<div class="form-inputs">
  <%= f.input :name %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "btn btn-primary" %>
  </div>
<% end %>

<div>
  <%= link_to "Back to lists", lists_path, class: "btn btn-secondary" %>
</div>
